{
  "features": {
    "currencyExtractor": true,
    "numberOfSentencesExtractor": true,
    "historicalFigureExtractor": true,
    "placesExtractor": true,
    "countryExtractor": true,
    "frequencyUniquenessWordsExtractor": true,
    "numberOfLongWordsExtractor": true,
    "averageWordLengthExtractor": true,
    "keyWordExtractor": true,
    "exceptKeyWordExtractor": true,
    "frequencyCommonWordExtractor": true,
    "capitalWordExtractor": true,
    "documentLengthExtractor": true
  },
  "longWordLength": 7,
  "numberOfOccurrences": 4,

  "proportionOfDataSets": [60, 40],
  "k": 3,
  "measure": "ngrams",
  "metric": "euclideanMetric",
  "ngramLimitation": [2, 3],

  "testMode": {
    "enable": true,
    "numberOfArticles": 5
  },
  "regex": "(?i)west-germany|usa|france|uk|canada|japan"
}